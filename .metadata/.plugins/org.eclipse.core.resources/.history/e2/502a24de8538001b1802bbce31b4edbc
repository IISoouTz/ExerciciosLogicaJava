package listaL;

public class listaL1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		int numero;
		
		for(numero=1;numero<=100;numero++) {
			System.out.println("Numero - " + numero);
		}
		
		for(numero=100;numero>=1;numero--) {
			System.out.println("Numero - " + numero);
		}
		
	}

}
